model Transfer {
  id         Int      @id @default(autoincrement())
  Group      String
  TransDate  DateTime
  DRNo       String
  Branch     String
  Department String
  Category   String
  Item       String
  Qty        Float
  Unit       String
  Price      Float
  Amount     Float
  createdAt  DateTime @default(now())
}
model WarehouseInv {
  id           Int      @id @default(autoincrement())
  item         String
  pcsKg        Float?
  grams        Float?
  price        Float?
  amount       Float?
  category     String?
  department   String?
  group        String?
  branch       String?
  unit         String?
  location     String?
  dateEncoded  DateTime?
  encodedBy    String?
  countedBy    String?
  checkedBy    String?
  createdAt    DateTime @default(now())
}
model User {
  id           Int      @id @default(autoincrement())
  username     String   @unique
  password     String
  email        String?
  firstName    String?
  lastName     String?
  branch       String?
  position     String?
  permissions  String?  // Can be a comma-separated string or use a relation for advanced permissions
  createdAt    DateTime @default(now())
}
