generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Purchase {
  id          Int      @id @default(autoincrement())
  Group       String
  InvoiceDate String
  InvoiceNo   String
  Supplier    String
  Category    String
  Item        String
  Qty         Float
  Unit        String
  Price       Float
  Amount      Float
  GramsQty    Float
  Branch      String
  createdAt   DateTime @default(now())
}


model Item {
  MID         Int       @id @default(autoincrement())
  MDate       DateTime?
  EKCode      String?   @db.VarChar(50)
  Barcode     String?   @db.VarChar(50)
  ItemCode    String?   @db.VarChar(50)
  FoodGroup   String?   @db.VarChar(50)
  FGCode      String?   @db.VarChar(50)
  Category    String?   @db.VarChar(50)
  FCCode      String?   @db.VarChar(50)
  Subcategory String?   @db.VarChar(50)
  FSCCode     String?   @db.VarChar(50)
  Department  String?   @db.VarChar(50)
  Supplier    String?   @db.VarChar(100)
  ItemDesc    String?   @db.VarChar(255)
  Item        String?   @db.VarChar(100)
  Brand       String?   @db.VarChar(100)
  UOMQty      Float?
  Unit        String?   @db.VarChar(20)
  CUOMQty     Float?
  CUnit       String?   @db.VarChar(20)
  Location    String?   @db.VarChar(50)
  Storage     String?   @db.VarChar(50)
  Container   String?   @db.VarChar(50)
  ForBarcode  Boolean?
  EncodedBy   String?   @db.VarChar(50)
  Status      String?   @db.VarChar(20)
  ForInv      Boolean?
  EkNum       String?   @db.VarChar(50)
  BGroup      String?   @db.VarChar(50)
  PCSPerPack  Int?
  PricePc     Float?
  Price       Float?
  UInv        String?   @db.VarChar(50)
}

